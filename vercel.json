{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/serenity/(.*)",
      "dest": "/serenity/$1"
    },
    {
      "src": "/hope/(.*)",
      "dest": "/hope/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "contact": "./api/contact.js"
  }
}