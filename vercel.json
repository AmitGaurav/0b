{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "serenity/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "staticGeneration": true
      }
    },
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/serenity",
      "dest": "/serenity/index.html"
    },
    {
      "src": "/serenity/(.*)",
      "dest": "/serenity/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/serenity/[^.]+",
      "dest": "/serenity/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}